FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      pcm2.solid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.5;

boundaryField
{
    floor
    {
	    type            zeroGradient;
    }

    ceiling
    {
        type            zeroGradient;
    }
	
	frontAndBack
    {
        type            empty;
    }
}


