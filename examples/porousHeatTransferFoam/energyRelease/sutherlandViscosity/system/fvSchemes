FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default         Gauss upwind;
//    div(phi,U)      Gauss upwind;
    div(phi,U)      Gauss linear;
//	div(phi,U)      Gauss linearUpwind grad(U);
//    div(phi,h.fluid)      Gauss upwind;
//    div(phi,h)      Gauss linear;
	div(phi,h.fluid)	Gauss linearUpwind grad(h.fluid);
}

laplacianSchemes
{
    default         Gauss linear orthogonal;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         orthogonal;
}


